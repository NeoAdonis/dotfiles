{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "final_space": true,
  "shell_integration": true,
  "patch_pwsh_bleed": true,
  "console_title_template": "{{ if .Root }}root | {{ end }}{{ if .Env._RazzleArguments }}{{ .Env._RazzleArguments }} | {{ end }}{{ .PWD }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "template": " {{ if .WSL }}WSL {{ end }}{{ if .Env._RazzleArguments }}ðŸŽ©ï¸Ž{{ end }}{{ .Icon }}  ",
          "foreground": "#ffffff",
          "background": "#2886de"
        },
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#2f4577",
          "powerline_symbol":"\ue0b0",
          "template": " {{ if .SSHSession }}\ueba9 @{{ .HostName }}{{ end }} "
        },
        {
          "type": "text",
          "style": "powerline",
          "template": " {{ .Env._BuildArch }}{{ .Env._BuildType }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#115ea3"
        },
        {
          "type": "path",
          "style": "powerline",
          "template": " \ue5ff {{ path .Path .Location }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#da3b01",
          "properties": {
            "folder_format": "<#dddddd>%s</>",
            "right_format": "<b>%s</b>",
            "display_root": true,
            "max_depth": 3,
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "template": " \ue702 {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf040{{ end }}{{ if .Staging.Changed }} \uf4a7{{ end }}{{ if gt .StashCount 0 }}  \uf51e {{ .StashCount }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#f1502f",
          "background_templates": [
            "{{ if .Working.Changed }}#8a3707{{ end }}",
            "{{ if .Staging.Changed }}#094509{{ end }}",
            "{{ if or (gt .Ahead 0) (gt .Behind 0) }}#6d2064{{ end }}"
          ],
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "ignore_status": [".*/os\\d*/src"],
            "tag_icon": "\uf412 "
          }
        },
        {
          "type": "bun",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#3C82F6",
          "template": "{{ if .Full }} ðŸ¥Ÿ {{ end }}"
        },
        {
          "type": "node",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#6ca35e",
          "template": "{{ if .Full }} \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ end }}",
          "properties": {
            "fetch_package_manager": true
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#00666d",
          "template": " .NET ",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "go",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#7fd5ea",
          "template": " \ue627 ",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " \ue606 {{ .Full }} {{ .Venv }} ",
          "foreground": "#ffffff",
          "background": "#3776ab",
          "properties": {
            "home_enabled": true
          }
        },
        {
          "type": "rust",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#817400",
          "template": " \ue7a8 ",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "text",
          "style": "plain",
          "template": ".",
          "foreground": "transparent",
          "background": "transparent"
        },
        {
          "type": "status",
          "style": "powerline",
          "properties": {
            "always_enabled": true
          },
          "template": " {{ if eq .Code 0 }}\uf00c{{ else }}\uf071 {{ reason .Code }}{{ end }} ",
          "foreground": "#ffffff",
          "background": "#107c10",
          "powerline_symbol": "\ue0b0",
          "leading_powerline_symbol": "\ue0d7",
          "background_templates": ["{{ if ne .Code 0 }}#c50f1f{{ end }}"]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "time",
          "foreground": "#bbbbbb",
          "style": "plain",
          "template": "{{ .CurrentDate | date .Format }}",
          "properties": {
            "time_format": "15:04:05"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "foreground_templates": ["{{ if .Root }}#fde300{{ end }}"],
          "template": "{{ if .Root }}\uf0e7{{ else }}>{{ end }}>"
        }
      ]
    }
  ],
  "secondary_prompt": {
    "template": " > ",
    "foreground": "#ffffff",
    "background": "transparent"
  }
}
