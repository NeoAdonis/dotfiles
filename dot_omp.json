{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": " > ",
    "foreground": "#cdd6f4",
    "background": "transparent"
  },
  "console_title_template": "{{ if .Root }}root | {{ end }}{{ if .Env._RazzleArguments }}{{ .Env._RazzleArguments }} | {{ end }}{{ .PWD }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "template": " {{ if .WSL }}WSL {{ end }}{{ if .Env._RazzleArguments }}ðŸŽ©ï¸Ž{{ end }}{{ .Icon }}  ",
          "foreground": "#dce0e8",
          "background": "#04a5e5",
          "type": "os",
          "style": "diamond"
        },
        {
          "template": " {{ if .SSHSession }}\ueba9 @{{ .HostName }}{{ end }} ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#1e66f5",
          "type": "session",
          "style": "powerline"
        },
        {
          "template": " {{ .Env._BuildArch }}{{ .Env._BuildType }} ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#115ea3",
          "type": "text",
          "style": "powerline"
        },
        {
          "options": {
            "display_root": true,
            "folder_format": "<#dddddd>%s</>",
            "max_depth": 3,
            "right_format": "<b>%s</b>",
            "style": "agnoster_short"
          },
          "template": " \ue5ff {{ path .Path .Location }} ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#fe640b",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "ignore_status": [
              ".*/os\\d*/src"
            ],
            "tag_icon": "\uf412 "
          },
          "template": " \ue702 {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf040{{ end }}{{ if .Staging.Changed }} \uf4a7{{ end }}{{ if gt .StashCount 0 }}  \uf51e {{ .StashCount }}{{ end }} ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#f1502f",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if .Working.Changed }}#87121c{{ end }}",
            "{{ if .Staging.Changed }}#40a02b{{ end }}",
            "{{ if or (gt .Ahead 0) (gt .Behind 0) }}#8839ef{{ end }}"
          ]
        },
        {
          "template": "{{ if .Full }} ðŸ¥Ÿ {{ end }}",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#3c82f6",
          "type": "bun",
          "style": "powerline"
        },
        {
          "options": {
            "fetch_package_manager": true
          },
          "template": "{{ if .Full }} \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ end }}",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#6ca35e",
          "type": "node",
          "style": "powerline"
        },
        {
          "options": {
            "fetch_version": false
          },
          "template": " .NET ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#00666d",
          "type": "dotnet",
          "style": "powerline"
        },
        {
          "options": {
            "fetch_version": false
          },
          "template": " \ue627 ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#7fd5ea",
          "type": "go",
          "style": "powerline"
        },
        {
          "options": {
            "home_enabled": true
          },
          "template": " \ue606 {{ .Full }} {{ .Venv }} ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#3776ab",
          "type": "python",
          "style": "powerline"
        },
        {
          "options": {
            "fetch_version": false
          },
          "template": " \ue7a8 ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#817400",
          "type": "rust",
          "style": "powerline"
        },
        {
          "template": ".",
          "foreground": "transparent",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "options": {
            "always_enabled": true
          },
          "template": " {{ if eq .Code 0 }}\uf00c{{ else }}\uf071 {{ reason .Code }}{{ end }} ",
          "foreground": "#dce0e8",
          "powerline_symbol": "\ue0b0",
          "background": "#40a02b",
          "type": "status",
          "style": "powerline",
          "leading_powerline_symbol": "\ue0d7",
          "background_templates": [
            "{{ if ne .Code 0 }}#d20f39{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "{{ if .Root }}\uf0e7{{ else }}>{{ end }}>",
          "foreground": "#cdd6f4",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Root }}#fab387{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "shell_integration": true,
  "final_space": true,
  "patch_pwsh_bleed": true
}
